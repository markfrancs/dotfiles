# General purpose
alias user='echo $USER'
alias shell='echo $SHELL'
alias path='echo $PATH'
alias home='cd $HOME'
alias cl='clear'
alias rst='reset'
alias all='ls -alF'
alias srczsh='source $HOME/.zshrc'
alias free='free -htw'
alias df='df -h'
alias srt='ln -s'


# Git
alias gcfg='git config --list'


